@model IEnumerable<MVC_CORE.Models.Job>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New Position", "Create", "Job")
</p>
<table class="table">
    <tr>
        <th>
            Job's Name
        </th>
        <th>
            Job's Description'
        </th>
        <th>
            Major
        </th>
        <th>
            Major Importance
        </th>
        <th>
            Skill
        </th>
        <th>
            Skill Level
        </th>
        <th>
            Skill Importance
        </th>
        <th>
            Position Created
        </th>
        <th> Pick Up Action</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JobName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Major)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MajorImp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Skill)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkillLevel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SkillImp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileCreated)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) <br />
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table><br />
<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">


        <li>@Html.ActionLink("My Profile Management Page", "CvmPlayer", "Company_Login")</li>
        <li>@Html.ActionLink("Show My List of Positions", "ShowAll", "Job")</li>
        <li>@Html.ActionLink("Create New Position", "Create", "Job")</li>

    </ul>
</div>